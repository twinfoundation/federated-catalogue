{
	"language": "en-GB",
	"ignorePaths": [
		"**/node_modules/**",
		"LICENSE",
		"**/coverage/**",
		"**/dist/**",
		"**/docs/reference/**",
		"**/docs/open-api/**",
		"**/tests/**",
		"*.yaml",
		"package.json",
		"rollup.config.mjs",
		"vitest.config.*",
		"tsconfig.json",
		"typedoc.json",
		".eslintrc.cjs",
		"**/wordlists/**",
		"**/*.json"
	],
	"words": [
		"bech",
		"abbrev",
		"Jsonld",
		"nameof",
		"openapi",
		"Stiftung",
		"Unrevoke",
		"ABNF",
		"EORI",
		"VATID",
		"GLEIF",
		"twinfoundation",
		"buildx"
	],
	"ignoreRegExpList": [
		"/from\\s+(['\"]).*\\1/",
		"/eslint-disable.*/",
		"/^```shell[\\s\\S]*?```$/gm",
		"/^```json[\\s\\S]*?```$/gm",
		"/tst1.*/"
	]
}
